	<queryString>
		<![CDATA[SELECT
    CONCAT(c.Nombre,' ',c.Apellido) AS Cliente,
    p.Nombre AS Postre,
    pe.Cantidad,
    pe.Costo,
    pe.FechaEntrega,
    te.Tipo AS Entrega
FROM
    pedidos pe
INNER JOIN
    clientes c ON pe.idCliente = c.idCliente
INNER JOIN
    postres p ON pe.idPostre = p.idPostre
INNER JOIN
    tipoentrega te ON pe.idTipoEntrega = te.idTipoEntrega
WHERE
    pe.FechaEntrega < CURRENT_DATE
ORDER BY
    pe.FechaEntrega DESC;]]>
	</queryString>
